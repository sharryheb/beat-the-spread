    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "create-react-app": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
